{
  "version": 2,
  "builds": [
    { "src": "server.js", "use": "@vercel/node@2" }
  ],
  "routes": [

    {
      "src": "/api/(.*)",
      "dest": "server.js"
    },
    {
      "src": "/health",
      "dest": "server.js"
    },
    {
      "src": "/test-(.*)",
      "dest": "server.js"
    },
    {
      "src": "/",
      "dest": "server.js"
    },
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ],
  "functions": {
    "server.js": {
      "maxDuration": 30
    }
  },
  "env": {
    "NODE_ENV": "production"
  }

    { "src": "/(.*)", "dest": "/server.js" }
  ]

}
