{
  "version": 2,
  "builds": [
    { 
      "src": "server.js", 
      "use": "@vercel/node@2" 
    }
  ],
  "routes": [
    { "src": "/health", "dest": "/server.js" },
    { "src": "/api/(.*)", "dest": "/server.js" },
    { "src": "/(.*)", "dest": "/server.js" }
  ],
  "functions": {
    "server.js": {
      "maxDuration": 30
    }
  },
  "buildCommand": "npm run vercel-build"
}
